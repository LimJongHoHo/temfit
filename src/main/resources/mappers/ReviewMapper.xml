<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="kr.co.limbin.temfit.mappers.ReviewMapper">
    <insert id="insert"
            useGeneratedKeys="true"
            keyColumn="id"
            keyProperty="id">
        INSERT INTO `temfit`.`reviews` (`article_id`, `user_email`, `content`, `created_at`, `modified_at`,
                                         `is_deleted`)
        VALUES (#{article.articleId}, #{article.userEmail}, #{article.content},
                #{article.createdAt}, #{article.modifiedAt}, #{article.isDeleted})
    </insert>

</mapper>